{
  "name": "restful-redux",
  "description": "Redux App",
  "scripts": {
    "start": "run-p start:dev",
    "server": "nodemon server/app.js",
    "start:dev": "webpack-dev-server --config ./client/webpack.config.dev.js --port 4000",
    "build": "webpack --config ./client/webpack.config.prod.js",
    "postbuild": "run-p server:build",
    "server:build": "node server/app.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "bootstrap": "4.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "cors": "",
    "express": "",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "immer": "2.1.3",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "",
    "prop-types": "15.7.2",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-notifications": "^1.4.3",
    "react-redux": "6.0.1",
    "react-router-dom": "5.0.0",
    "react-spinners": "^0.6.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "redux-undo": "^0.6.1",
    "reselect": "^4.0.0",
    "slug": "^1.1.0",
    "svg-inline-loader": "^0.8.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-preset-react-app": "7.0.2",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "enzyme": "3.9.0",
    "nodemon": "",
    "eslint": "5.15.2",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-react": "7.12.4",
    "fetch-mock": "7.3.1",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.5.0",
    "node-fetch": "^2.3.0",
    "npm-run-all": "4.1.5",
    "postcss-loader": "3.0.0",
    "redux-immutable-state-invariant": "2.1.0",
    "style-loader": "0.23.1",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "3.1.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1"
  },
  "engines": {
    "node": ">=8"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "env": {
      "NODE_ENV": "development",
      "PORT": 5000
    }
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ],
    "plugins": [
      "emotion"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  }
}
